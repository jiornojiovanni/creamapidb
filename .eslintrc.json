{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "indent": ["error", 4],
        "no-prototype-builtins": 0,
        "no-plusplus": 0,
        "max-classes-for-file": 5
    }
}
